{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es2016",
      "dom"
    ],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "types": [
      "node"
    ],
    "declaration": true,
    "outDir": "./build",
    "rootDir": "./src",
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/*",
    "src/examples/racing",
    "src/data/*"
  ]
}